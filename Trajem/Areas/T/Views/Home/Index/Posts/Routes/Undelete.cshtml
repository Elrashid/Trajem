  <script>
    var contacts_Undelete_View;
    g_Router.route("/Contacts/Undelete/:contact", function (contact, params) {
      if (DEBUG) { console.log("Breadcrumb Start !"); }
      g_Breadcrumb_Show([
      [g_B_T_ERP,  String.format("javascript:g_Router.navigate('/')")],
      [g_B_T_Contacts, String.format("javascript:g_Router.navigate('/Contacts')")],
        [g_T_Globale_Undelete, ""],
      ]);
      if (DEBUG) { console.log("Breadcrumb End !"); }
      if (DEBUG) { console.log("View initialization Start !"); }
      try { contacts_Undelete_View.destroy(); } catch (e) { if (DEBUG) { console.log(e) } }
        contacts_Undelete_View = new kendo.View(
                   'contacts_Undelete_Template',
               {
                   model: { id: contact }, evalTemplate: true,
                   show: function () { contacts_Undelete_Route_Event_Show(); },
                   hide: function () { contacts_Undelete_Route_Event_Hide(); }
               });
        g_Var_Current_View = contacts_Undelete_View;
        if (DEBUG) { console.log("View initialization End !"); }
        if (DEBUG) { console.log("View show In Start !"); }
        g_Layout.showIn("#g_App_Body", contacts_Undelete_View, "swap");
        if (DEBUG) { console.log("View show In End !"); }
    });
  

    function contacts_Undelete_Route_Event_Show() {
        if (DEBUG) { console.log("View Show Event Start !"); }

        if (DEBUG) { console.log("View Show Event End !"); }
    }
     
    function contacts_Undelete_Route_Event_Hide() {
        if (DEBUG) { console.log("View Hide Event Start !"); }
        if (g_Var_Current_View != contacts_View) {
            contacts_Undelete_View.destroy();
            if (DEBUG) { console.log("The view is destroyed !!"); }
        }
        if (DEBUG) { console.log("View Hide Event End !"); }

    }
</script>  
